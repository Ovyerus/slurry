<Window x:Class="Slurry.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Slurry"
        xmlns:Controls="clr-namespace:Slurry.Controls"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Slurry" Height="230" Width="270">
    <Grid>
        <TabControl x:Name="tabControl" Margin="0,0,0,-1.634">
            <TabItem Header="Backup">
                <Controls:Backup/>
            </TabItem>
            <TabItem Header="Restore">
                <Controls:Restore/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
